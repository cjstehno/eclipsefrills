<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         point="org.eclipse.ui.viewActions">
      <viewContribution
            id="net.sourceforge.eclipsefrills.workingset.viewcontribution"
            targetID="org.eclipse.search.ui.views.SearchView">
         <action
               class="net.sourceforge.eclipsefrills.workingset.action.SelectWorkingSetForResultsAction"
               icon="icons/workset.gif"
               id="net.sourceforge.eclipsefrills.workingset.selectworkingset"
               label="%eclipsefrills.workingset.action.select.label"
               menubarPath="additions"
               toolbarPath="additions"
               tooltip="%eclipsefrills.workingset.action.select.tooltip">
         </action>
      </viewContribution>
   </extension>
   <extension
         point="org.eclipse.help.toc">
      <toc
            file="toc.xml"
            primary="true">
      </toc>
      <toc
            file="toctasks.xml">
      </toc>
   </extension>

</plugin>
